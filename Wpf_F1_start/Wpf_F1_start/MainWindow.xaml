<Window x:Class="Wpf_F1_start.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_F1_start"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="450"
        Width="800"
        Background="White">
    <Grid>
        <Ellipse Name="circle6"
                 HorizontalAlignment="Left"
                 Height="100"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2"
                 Margin="308,10,0,0" />
        <Ellipse Name="circle2"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="108,10,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />

        <Ellipse Name="circle3"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="208,110,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Ellipse Name="circle7"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="408,110,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Ellipse Margin="208,10,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 StrokeThickness="2"
                 Stroke="Black"
                 Name="circle4"
                 Height="100"
                 HorizontalAlignment="Left"
                 Fill="gray" />

        <Ellipse Name="circle1"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="108,110,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 StrokeThickness="2"
                 Stroke="Black" />
        <Ellipse Name="circle5"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="308,110,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Button Name="Stop"
                Content="Stop"
                HorizontalAlignment="Left"
                Margin="315,329,0,0"
                VerticalAlignment="Top"
                Width="80"
                Height="50"
                FontSize="20"
                Background="IndianRed"
                Click="Stop_Click" />
        <Button Name="Start"
                Content="Start"
                HorizontalAlignment="Left"
                Margin="158,339,0,0"
                VerticalAlignment="Top"
                Background="LimeGreen"
                Width="60"
                Height="40"
                FontSize="20"
                Click="Start_Click" />

        <Button Name="Reset"
                Content="Reset"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Width="60"
                Height="40"
                FontSize="20"
                Margin="477,339,0,0"
                Click="Reset_Click" />
        <TextBlock Name="Stopwatch"
                   HorizontalAlignment="Left"
                   Margin="154,242,0,0"
                   TextWrapping="Wrap"
                   Text=""
                   VerticalAlignment="Top"
                   Height="35"
                   Width="408"
                   FontSize="30"
                   TextAlignment="Center" />
        <Ellipse Name="circle8"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="408,10,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Ellipse Name="circle9"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="508,110,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Ellipse Name="circle10"
                 HorizontalAlignment="Left"
                 Height="100"
                 Margin="508,10,0,0"
                 VerticalAlignment="Top"
                 Width="100"
                 Fill="gray"
                 Stroke="Black"
                 StrokeThickness="2" />
        <Label Content="Best time:"
               HorizontalAlignment="Left"
               Margin="687,311,0,0"
               VerticalAlignment="Top" />
        <TextBlock Name="BestTime"
                   HorizontalAlignment="Left"
                   Margin="640,354,0,0"
                   TextWrapping="Wrap"
                   Text=""
                   VerticalAlignment="Top"
                   Width="150"
                   Height="25"/>

    </Grid>
</Window>
